{
  "include": [
    "src",
  ],

  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    ".venv",
  ],

  "venvPath": ".",
  "venv": ".venv",

  "ignore": [
    "src/oldstuff"
  ],

  "reportMissingImports": true,
  "reportMissingTypeStubs": false,

  "executionEnvironments": [
    {
      "root": "src"
    }
  ]
}
